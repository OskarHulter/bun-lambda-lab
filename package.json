{
	"name": "bun-lambda-lab",
	"module": "index.ts",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "convex dev",
		"start": "wrangler dev",
		"serve": "ladle serve -p 61110",
		"serve-prod": "ladle preview -p 61110",
		"build-preview": "ladle build && ladle preview -p 61110",
		"build": "ladle build",
		"lint": "echo 'no lint'",
		"test-dev": "TYPE=dev bun exec playwright test",
		"test-prod": "TYPE=prod bun exec playwright test",
		"test": "bun test-dev && bun test-prod",
		"import": "convex import tasks sampleData.jsonl",
		"test:update": "TYPE=update bun exec playwright test -u",
		"clean-all": "bun clean-dist & bun clean-deps",
		"clean-dist": "rm -rf ./packages/*/dist & rm -rf ./packages/*/tsconfig.tsbuildinfo & rm -rf ./apps/*/dist & rm -rf ./apps/*/.next",
		"clean-deps": "rm -rf ./packages/*/node_modules & rm -rf ./apps/*/node_modules & rm -rf ./node_modules"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230419.0",
		"@playwright/test": "^1.39.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.8.7",
		"bun-types": "latest",
		"wallace-cli": "^3.0.0",
		"wrangler": "^3.14.0"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"@half0wl/money": "^1.0.2",
		"@klinecharts/pro": "^0.1.1",
		"@ladle/react": "^3.2.2",
		"@tanstack/react-form": "^0.4.2",
		"chart.js": "^4.4.0",
		"chartist": "^1.3.0",
		"chartjs-plugin-a11y-legend": "^0.1.1",
		"chartjs-plugin-annotation": "^3.0.1",
		"chartjs-plugin-autocolors": "^0.2.2",
		"chartjs-plugin-colorschemes": "^0.4.0",
		"chartjs-plugin-datalabels": "^2.2.0",
		"chartjs-plugin-gradient": "^0.6.1",
		"chartjs-plugin-style": "^0.5.0",
		"chartjs-plugin-zoom": "^2.0.1",
		"convex": "^1.4.1",
		"date-fns": "^2.30.0",
		"hono": "^3.8.2",
		"htmx.org": "^1.9.6",
		"indicatorts": "^1.0.16",
		"klinecharts": "^9.5.4",
		"lucia": "^2.7.2",
		"nodejs-polars": "^0.8.2",
		"open-props": "^1.6.8",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"sync-fetch": "^0.5.2",
		"talib": "^1.1.5",
		"trading-signals": "^3.7.0",
		"ts-pattern": "^5.0.5",
		"unstorage": "^1.9.0"
	}
}
