{
  "name": "bun-lambda-lab",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "serve": "ladle serve -p 61110",
    "serve-prod": "ladle preview -p 61110",
    "build-preview": "ladle build && ladle preview -p 61110",
    "build": "ladle build",
    "lint": "echo 'no lint'",
    "test-dev": "TYPE=dev bun exec playwright test",
    "test-prod": "TYPE=prod bun exec playwright test",
    "test": "bun test-dev && bun test-prod",
    "test:update": "TYPE=update bun exec playwright test -u"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.7",
    "bun-types": "latest",
    "wallace-cli": "^3.0.0",
    "wrangler": "^3.14.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ladle/react": "^3.2.2",
    "@tanstack/react-form": "^0.4.2",
    "chartist": "^1.3.0",
    "convex": "^1.4.1",
    "htmx.org": "^1.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sync-fetch": "^0.5.2"
  }
}
